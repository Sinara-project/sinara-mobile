<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/md_theme_onBackground"
    android:backgroundTint="@color/md_theme_onBackground"
    tools:context=".ui.configuration.Configuration">

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="76dp"
        android:layout_height="97dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="32dp"
        android:src="@drawable/sinara_logo_home"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="17dp"
        android:layout_marginTop="21dp"
        android:fontFamily="@font/montserrat_bold"
        android:text="Configurações"
        android:textColor="@color/md_theme_primaryContainer"
        android:textSize="60sp"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/imageView9" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="23dp"
        android:layout_height="45dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="154dp"
        android:src="@drawable/bt_voltar"
        android:textColor="@color/md_theme_primaryContainer"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="49dp"
        android:layout_marginTop="28dp"
        android:src="@drawable/bt_camera"
        android:textColor="@color/md_theme_primaryContainer"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView19" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="49dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/bt_sms"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView27" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="49dp"
        android:layout_marginTop="9dp"
        android:fontFamily="@font/montserrat_medium"
        android:text="Permissão de câmera:"
        android:textColor="@color/md_theme_primaryContainer"
        android:textSize="40sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView11" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="49dp"
        android:layout_marginTop="9dp"
        android:fontFamily="@font/montserrat_medium"
        android:text="Permissão de sms:"
        android:textColor="@color/md_theme_primaryContainer"
        android:textSize="40sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView12" />

    <Button
        android:id="@+id/button11"
        android:layout_width="589dp"
        android:layout_height="76dp"
        android:layout_marginStart="49dp"
        android:layout_marginBottom="20dp"
        android:fontFamily="@font/montserrat_bold"
        android:text="Cadastrar novo rosto"
        android:textSize="40dp"
        app:cornerRadius="10dp"
        app:icon="@drawable/ic_cadastrar_rosto"
        app:iconSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/button12"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button12"
        android:layout_width="477dp"
        android:layout_height="76dp"
        android:layout_marginStart="48dp"
        android:layout_marginBottom="49dp"
        android:fontFamily="@font/montserrat_bold"
        android:text="Alterar senha"
        android:textSize="40dp"
        app:cornerRadius="10dp"
        app:icon="@drawable/ic_alterar_senha"
        app:iconSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="96dp"
        android:backgroundTint="@color/md_theme_secondaryContainer"
        android:fontFamily="@font/montserrat_bold"
        android:padding="2dp"
        android:text="ATIVAR"
        android:textSize="40sp"
        app:cornerRadius="10dp"
        app:layout_constraintStart_toEndOf="@+id/textView27"
        app:layout_constraintTop_toBottomOf="@+id/textView19" />

    <Button
        android:id="@+id/button15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="76dp"
        android:backgroundTint="#7FD170"
        android:fontFamily="@font/montserrat_bold"
        android:padding="2dp"
        android:text="OK"
        android:textColor="#409346"
        android:textSize="40sp"
        app:cornerRadius="10dp"
        app:layout_constraintStart_toEndOf="@+id/textView30"
        app:layout_constraintTop_toBottomOf="@+id/button14" />


</androidx.constraintlayout.widget.ConstraintLayout>