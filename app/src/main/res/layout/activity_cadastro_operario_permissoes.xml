<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#0B1B54"
    tools:context=".cadastro.operario.CadastroOperarioPermissoes">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="268dp"
        android:layout_height="71dp"
        android:layout_marginTop="226dp"
        android:fontFamily="@font/montserrat_medium"
        android:gravity="center"
        android:letterSpacing="@dimen/material_emphasis_disabled_background"
        android:text="Permissões"
        android:textColor="@color/white"
        android:textSize="28sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/bt_salvar"
        android:layout_width="330dp"
        android:layout_height="70dp"
        android:layout_marginBottom="28dp"
        android:fontFamily="@font/montserrat_bold"
        android:text="Salvar"
        android:textColor="@android:color/white"
        android:textSize="25sp"
        app:backgroundTint="#384994"
        app:cornerRadius="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/bt_voltar3"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="100dp"
        android:background="@android:color/transparent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.091"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bt_voltar"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="163dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="65dp"
        android:src="@drawable/icone_tela_opcoes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@+id/formsCap"
        android:layout_width="331dp"
        android:layout_height="51dp"
        android:layout_marginTop="36dp"
        android:background="#8594D4"
        android:fontFamily="@font/montserrat_medium"
        android:gravity="center_vertical|end"
        android:text="Forms de Captação  "
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <CheckBox
        android:id="@+id/formsCap2"
        android:layout_width="331dp"
        android:layout_height="51dp"
        android:background="#8594D4"
        android:fontFamily="@font/montserrat_medium"
        android:gravity="center_vertical|end"
        android:text="Forms de Pré-Tratamento  "
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/formsCap" />

    <CheckBox
        android:id="@+id/formsCap3"
        android:layout_width="331dp"
        android:layout_height="51dp"
        android:background="#8594D4"
        android:fontFamily="@font/montserrat_medium"
        android:gravity="center_vertical|end"
        android:text="Forms de Tratamento Prim.  "
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/formsCap2" />

    <CheckBox
        android:id="@+id/formsCap4"
        android:layout_width="331dp"
        android:layout_height="51dp"
        android:background="#8594D4"
        android:fontFamily="@font/montserrat_medium"
        android:gravity="center_vertical|end"
        android:text="Forms de Tratamento Sec.  "
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/formsCap3" />

    <CheckBox
        android:id="@+id/formsCap5"
        android:layout_width="331dp"
        android:layout_height="51dp"
        android:background="#8594D4"
        android:fontFamily="@font/montserrat_medium"
        android:gravity="center_vertical|end"
        android:text="Forms de Tratamento Ter.  "
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/formsCap4" />

    <CheckBox
        android:id="@+id/formsCap6"
        android:layout_width="331dp"
        android:layout_height="51dp"
        android:background="#8594D4"
        android:fontFamily="@font/montserrat_medium"
        android:gravity="center_vertical|end"
        android:text="Forms de Distribuição  "
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/formsCap5" />

</androidx.constraintlayout.widget.ConstraintLayout>