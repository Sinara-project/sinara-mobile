<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#0B1B54"
    tools:context=".cadastro.operario.CadastroOperarioSucesso">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="280dp"
        android:layout_height="67dp"
        android:layout_marginTop="226dp"
        android:fontFamily="@font/montserrat_bold"
        android:gravity="left"
        android:letterSpacing="@dimen/material_emphasis_disabled_background"
        android:text="Operario cadastrado!"
        android:textColor="@color/white"
        android:textSize="28sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="298dp"
        android:layout_height="226dp"
        android:layout_marginTop="39dp"
        android:fontFamily="@font/montserrat_medium"
        android:gravity="left"
        android:letterSpacing="@dimen/material_emphasis_disabled_background"
        android:text="Um e-mail com as informações de cadastro foi enviado ao endereço cadastrado."
        android:textColor="@color/white"
        android:textSize="28sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="163dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="65dp"
        android:src="@drawable/icone_tela_opcoes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>